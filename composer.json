{
  "name": "yasmf/festiplan",
  "config": {
    "vendor-dir": "/festiplan/lib/vendor"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/the-yasmf-project/yasmf"
    }
  ],
  "autoload": {
    "psr-4": {
      "controleurs\\": "/festiplan/controleurs",
      "modeles\\": "/festiplan/modeles",
      "application\\": "/festiplan/application"
    }
  },
  "require": {
    "php": ">=8.0",
    "phpunit/phpunit": "10.0.*",
    "ext-pdo": "*",
    "yasmf/yasmf": ">=1.6.1"
  },
  "require-dev": {
    "phpstan/phpstan": "*"
  }
}
